{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "src/*": [
        "src/*"
      ],
      "app/*": [
        "*"
      ],
      "components/*": [
        "src/components/*"
      ],
      "composables/*": [
        "src/composables/*"
      ],
      "layouts/*": [
        "src/layouts/*"
      ],
      "pages/*": [
        "src/pages/*"
      ],
      "assets/*": [
        "src/assets/*"
      ],
      "boot/*": [
        "src/boot/*"
      ],
      "modules/*": [
        "src/modules/*"
      ],
      "services/*": [
        "src/services/*"
      ],
      "utilities/*": [
        "src/utilities/*"
      ],
      "stores/*": [
        "src/stores/*"
      ],
      "vue$": [
        "node_modules/vue/dist/vue.runtime.esm-bundler.js"
      ]
    },
    "jsx": "preserve"
  },
  "exclude": [
    "dist",
    ".quasar",
    "node_modules"
  ]
}
